<h2>placeholder for list view</h2>
<table class="table table-responsive" sytle = "float:right">
<thead class="thead-default">
  <tr>
    <th style= "text-align:left">File Title</th>
    <th style= "text-align:left">File Link</th>
    <th style= "text-align:left" colspan="3">Actions</th>
  </tr>
  </thead>
  {{#each uploads as |upload|}}
  <tbody>
    <tr>
      <td>{{upload.title}}</td>
      <td><a href ="{{upload.url}}">{{upload.url}}</td>
        <td>
          <button type="button" class="btn btn-default">
            <span class="glyphicon glyphicon-download" aria-hidden="true"></span>
          </button>
        </td>
      <td>
        <button type="button" class="btn btn-default">
          <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        </button>
      </td>
      <td>
        <button type="button" class="btn btn-default">
          <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
        </button>
      </td>
    </tr>
  </tbody>
  {{/each}}



</table>
